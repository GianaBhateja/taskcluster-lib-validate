{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".js$"
  },
  "plugins": [
    "node_modules/jsdoc-babel",
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true
  },
  "opts": {
    "destination": "docs/",
    "tutorials": "docs/tutorials",
    "pedantic": true,
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/minami"
  }
}
